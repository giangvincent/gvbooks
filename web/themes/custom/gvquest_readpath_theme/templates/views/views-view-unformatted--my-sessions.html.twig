{#
  Custom row wrapper for the "my_sessions" view.
#}

{% if rows|length %}
  <div class="grid gap-12">
    {% for row in rows %}
      <article{{ row.attributes.addClass('sana-card overflow-hidden') }}>
        {{ row.content }}
      </article>
    {% endfor %}
  </div>
{% else %}
  <div class="rounded-3xl border border-dashed border-white/10 bg-sana-card/40 p-12 text-center text-sana-muted">
    {{ 'No sessions recorded yet.'|t }}
  </div>
{% endif %}
